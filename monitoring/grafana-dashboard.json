{
  "title": "MediRoute Overview",
  "schemaVersion": 38,
  "panels": [
    {
      "type": "stat",
      "title": "Uptime",
      "targets": [
        { "expr": "up{job=\"mediroute\"}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Request Rate",
      "targets": [
        { "expr": "sum(rate(http_server_requests_seconds_count{job=\"mediroute\"}[5m])) by (status)" }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency p95",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=\"mediroute\"}[5m])) by (le))" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Optimizer Job Duration p95",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(optimizer_job_duration_seconds_bucket[5m])) by (le))" }
      ]
    },
    {
      "type": "timeseries",
      "title": "JVM Memory Used",
      "targets": [
        { "expr": "jvm_memory_used_bytes{area=\"heap\"}" }
      ]
    }
  ]
}

